  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ENV=production nest start",
    "start:dev": "ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "ENV=testing jest --detectOpenHandles",
    "test:watch": "ENV=testing jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "ENV=testing jest --config ./test/jest-e2e.json"
  },
